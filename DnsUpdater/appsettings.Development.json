{
	"$schema": "https://raw.githubusercontent.com/dan3988/DnsUpdater/master/DnsUpdater/appsettings.schema.json",
	"Config": {
		"IpProvider": "http://checkip.amazonaws.com/",
		"Actions": [
			{
				"Location": "https://postman-echo.com/post",
				"Method": "POST",
				"Headers": {
					"Content-Type": "application/json"
				},
				"Body": "{\"IP\":\"${IP}\",\"OLDIP\":\"${OLDIP}\",\"DATE\":\"${DATE}\""
			}
		]
	}
}
