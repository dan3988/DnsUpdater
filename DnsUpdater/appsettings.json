{
	"$schema": "https://raw.githubusercontent.com/dan3988/DnsUpdater/master/DnsUpdater/appsettings.schema.json",
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.Hosting.Lifetime": "Information"
		},
		"Serilog": {
			"PathFormat": "logs\\{Date}.log"
		},
		"EventLog": {
			"Loglevel": {
				"Default": "Warning"
			}
		}
	},
	"BaseDir": "%ALLUSERSPROFILE%\\DnsUpdater"
}
