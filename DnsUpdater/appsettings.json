{
	"$schema": "./appsettings.schema.json",
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.Hosting.Lifetime": "Information"
		},
		"Serilog": {
			"PathFormat": "%AppData%\\DnsUpdater\\logs\\{Date}.log"
		},
		"EventLog": {
			"Loglevel": {
				"Default": "Warning"
			}
		}
	}
}
