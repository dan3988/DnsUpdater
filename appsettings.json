{
  "Logging": {
    "LogLevel": {
        "Default": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
    },
    "Serilog": {
        "PathFormat": "logs\\{Date}.log"
    },
    "EventLog": {
      "SourceName": "DnsUpdater",
      "Loglevel": {
        "Default": "Information"
      }
    }
  },
  "Config": {
    "IpProvider": "http://checkip.amazonaws.com/",
    "ChangeDelay": 10000,
    "Ignore": [
    ],
    "HistoryFile": "%USERPROFILE%\\Documents\\ip_history.log"
  }
}
